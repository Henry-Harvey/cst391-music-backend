{
  "name": "colorcollectionexpress",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start1": "nodemon -e js,ts --watch ./index.js --watch app --ignore 'lib/*' --exec 'npm run build && node ./index.ts' ",
    "debug1": "nodemon -e js,ts --watch ./index.js --watch app --ignore 'lib/*' --exec 'npm run build && node --inspect ./index.ts' ",
    "start": "nodemon ./index.ts",
    "debug": "nodemon --inspect ./index.ts",
    "build": "babel app --out-dir lib/app --extensions \".ts,.tsx\" --source-maps inline"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "mysql": "^2.18.1",
